import re
B=C=0
T,I,*Y=str.split,{}
F,*Q=exec,('in',[1,4001]*4)
for l in open(0):
 if' '<l:n,I[n]=T(l,'{');Y+=[[*map(int,re.findall('\d+',l))]]*(n<' ')
for p,L in Q:
 if'B'>p:F(f"C+={'(%d-%d)*'*4%(*L,)}1\nfor x in Y:i=-1;B+=sum(x)*all(L[i+1]<=v<L[i:=i+2]for v in x)")
 if p in I:
  for X in T(I[p][:-2],','):*r,R=T(X,':');Q+=(R,K:=L*1),;r>[]!=F('l=ord(X[0])//3%5*2^(">"in X);L[l]=K[l^1]=l%2+'+r[0][2:])
print(B,C)