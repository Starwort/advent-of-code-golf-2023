from heapq import*
I=*open(0),
for P in 0,1:
 *Q,V,C=(0,)*5,set(),{}
 while Q:
  R=heappop(Q);c,x,y,h,v=R
  if R[1:]in V:continue
  if x>139<y:print(c);break
  V.add(R[1:])
  for h,v in{(0,1),(0,-1),(1,0),(-1,0)}-{(h,v),(-h,-v)}:
   n,s,t=c,x,y
   for d in range(1,11):
    s+=h;t+=v
    if-1<s<141>t>-1:
     n+=int(I[t][s])
     if(d<4)^P:C[s,t,h,v]=n;heappush(Q,(n,s,t,h,v))