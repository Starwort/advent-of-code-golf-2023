from heapq import*
I=*open(0),
for P in 0,1:
 *Q,V=(0,)*5,{}
 while Q:c,x,y,h,v=R=heappop(Q);[[n:=c,[[n:=n+int(I[t][s]),(d<4)!=P!=heappush(Q,(n,s,t,h,v))]for d in range(1,11)if-(R[1:]in V)>-1<(s:=x+h*d)<141>(t:=y+v*d)>-1]]for h,v in[(0,1),(v,h),(1,0),(-v,-h)][h+v::2]];V[R[1:]]=x>139<y!=(Q:=print(c))