*I,=open(c:=0)
R=range(100)
D={}
S=lambda:print(sum((len(I)-y)*I[y].count('O')for y in R))
while c<1e9:
 for d in 0,1,2,3:
  *I,=map(list,I)
  for x in R:
   p=0
   for y in R:
    if'.'<I[y][x]:I[y][x]='.';I[p][x]='O';p+=1
    if'.'>I[y][x]:p=y+1
  if c<1>d:S()
  *I,=zip(*I[::-1])
 s=str(I);c+=1;d=c-D.get(s,-1e9);c+=(1e9-c)//d*d;D[s]=c
S()