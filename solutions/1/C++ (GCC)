int main(){system(m);ifstream d("d");stringstream b;b<<d.rdbuf();auto m=b.str();while(cin.get(r)||(r='\n'&&(a--))){r=toupper(r);if(r<'!'){s1+=ff*10+fl;s2+=sf*10+sl;s=33;ff=fl=sf=sl=0;continue;}k=(k=p>>('Z'-r))&1?__builtin_popcountll(k):0;s=m[s*24+k]-'0';if(s<18){sl=(s+2)/2;if(s%2)fl=sl;ff=ff?ff:fl;sf=sf?sf:sl;}};cout<<s1<<" "<<s2;}