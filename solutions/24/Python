from sympy import*
v,w,y,x,*z=var(":6"),[],0,0
for l in open(0):
 m,n,_,o,p,_=a=eval(l.replace(*'@,'));w+=[a]
 if x<3:x+=1;t=var("t"*x);z+=[v[i]+v[i+3]*t-a[i]-t*a[i+3]for i in(0,1,2)]
 for a,b,c,d,e,_ in w:k=n-p/o*m;i=(b-e/d*a-k)/(p/o-e/d+1/c);j=p/o*i+k;y+=(p*j>p*n)*(e*j>e*b)*i//2e14==j//2e14==1
print(y,sum(map(solve(z)[0].get,v[:3])))