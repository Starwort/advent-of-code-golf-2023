from sympy import*
v,y,*z=var(":9"),0;x,*w=-3,
for l in open(0):
 m,n,_,o,p,_=a=eval(l.replace(*'@,'));w+=a,;z+=[v[i-3]-a[i-3]-v[x]*(a[i]-v[i])for i in-x*b''];x+=1
 for a,b,c,t,e,_ in w:k=n-p/o*m;i=(b-e/t*a-k)/(p/o-e/t+1/c);j=p/o*i+k;y+=(p*j>p*n)+(e*j>e*b)>i/2e14>j//2e14>0
print(y,sum(map(solve(z)[0].get,v[:3])))