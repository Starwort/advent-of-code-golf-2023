import re
P={}
*b,=open(0)
x=range(140)
D=-1,0,1
for r,R in enumerate(b):
 for m in re.finditer(r'\d+',R):
  for c in{(r+s,c+d)for s in D for d in D for c in range(*m.span())}&{(r,c)for r in x for c in x if(b[r][c]in'0123456789.')^1}:P[c]=P.get(c,[])+[int(m[0])]
print(sum(map(sum,t:=P.values())),sum(p[0]*p[1]for p in t if len(p)>1))