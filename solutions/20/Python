I={}
q=1
O=L=F=0
D={}
for i in open(0):
 n,_,*d=i.replace(*', ').split();I[z:=n[1:3]]=[n[0],0,d]
 if'rx'in d:G=n[1:]
 for x in d:D[x]={**D.get(x,{}),z:0}
C={*D[G]}
while C:
 I['ro'][:2]=['%',1];F==1e3==print(O*L);F+=1;Q=[['ro',0,'']]
 for e,p,o in Q:
  O+=1-p;L+=p;t,s,l=I.get(e,'$$$')
  if(e in C)>p:q*=F;C-={e}
  if'%'<t:D[e][o]=p;s=all(D[e].values())
  if(t<'&')>p or'%'<t:s=1-s;I[e][1]=s;Q+=[[d,s,e]for d in l]
print(q)