I={}
q=1
O=L=F=0
for i in open(0):
 n,_,*d=i.replace(*', ').split();I[n[1:3]]=[n[0],0,d]
 if'rx'in d:G=n[1:]
D={k:{j:0for j in I if k in I[j][2]}for k in I}
C={*D[G]}
while F<1e4:
 O+=1;Q=[[d,0,'ro']for d in I['ro'][2]]
 for e,p,o in Q:
  O+=1-p;L+=p;t,s,l=I.get(e,'$$$')
  if(e in C)>p:q*=-~F;C-={e}
  if'%'<t:D[e][o]=p;s=all(D[e].values())
  if(t<'&')>p or'%'<t:s=1-s;I[e][1]=s;Q+=[[d,s,e]for d in l]
 F==999==print(O*L);F+=1
print(q)